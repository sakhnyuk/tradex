{
  "name": "tradex",
  "version": "0.0.1",
  "description": "Crossplatform crypto trading platform (Mac / PC) ",
  "repository": {
    "type": "git",
    "url": "https://github.com/sakhnyuk/tradex.git"
  },
  "main": "./src/dist/main.prod.js",
  "scripts": {
    "build": "concurrently \"yarn build-main\" \"yarn build-renderer\"",
    "build-main": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.main.prod.config --colors",
    "build-renderer": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.renderer.prod.config.js --colors",
    "start-renderer-dev": "webpack-dev-server --config ./webpack/webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config ./webpack/webpack.main.config.js && electron ./src/dist/main.dev.js",
    "dev": "rimraf src/dist && cross-env START_HOT=1 NODE_ENV=development npm run start-renderer-dev",
    "package-all": "yarn build && electron-builder build -mwl",
    "package-mac": "yarn build && electron-builder build --mac",
    "package-linux": "yarn build && electron-builder build --linux",
    "package-win": "yarn build && electron-builder build --win --x64",
    "electron-rebuild": "electron-rebuild --parallel --force --types prod,dev,optional",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "cryptocurrency",
    "terminal",
    "trading"
  ],
  "author": "Mikhail Sakhnyuk <its545@yandex.ru>",
  "license": "ISC",
  "devDependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@types/classnames": "^2.2.10",
    "@types/lodash": "^4.14.157",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-virtualized": "^9.21.10",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "concurrently": "^5.2.0",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "electron": "^12.1.0",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^1.1.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^2.0.1",
    "react-hot-loader": "^4.12.20",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.6",
    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "electron-log": "^4.2.2",
    "electron-window-state": "^5.0.3",
    "history": "^4.10.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-virtualized": "^9.21.2",
    "reconnecting-websocket": "^4.4.0",
    "redux": "^4.0.5",
    "redux-act": "^1.8.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0"
  },
  "build": {
    "publish": {
      "provider": "github"
    },
    "productName": "Tradex App",
    "appId": "com.tradex.org",
    "files": [
      "package.json",
      "src/dist/",
      "src/charting_library/",
      "src/assets/"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "icon": "./resources/icon.png",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "resources/ico-app.png",
      "signingHashAlgorithms": [
        "sha1",
        "sha256"
      ],
      "publisherName": [
        "tradex"
      ]
    },
    "nsis": {
      "oneClick": "true",
      "perMachine": "true"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "assets",
      "output": "release"
    }
  },
  "homepage": "./"
}
